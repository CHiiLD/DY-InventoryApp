<UserControl x:Class="R54IN0.WPF.InOutStockManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        xmlns:local="clr-namespace:R54IN0.WPF"
        Height="Auto" Width="Auto">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="2"/>
            <ColumnDefinition Width="229*"/>
        </Grid.ColumnDefinitions>

        <local:Finder x:Name="Finder" />
        <GridSplitter HorizontalAlignment="Stretch" Grid.Column="1" />
        <local:InOutStockDataGrid x:Name="IOStockDataGrid" Grid.Column="2" />
    </Grid>
</UserControl>
